  ]
  include_dirs = [
    "//llvm/lib/Target/X86",
    "//llvm/tools/llvm-exegesis/lib",
  ]
  sources = [
    "AnalysisTest.cpp",
    "AssemblerTest.cpp",
    "BenchmarkResultTest.cpp",
    "RegisterAliasingTest.cpp",
    "SnippetGeneratorTest.cpp",
    "TargetTest.cpp",
