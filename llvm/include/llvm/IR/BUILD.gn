import("//llvm/utils/TableGen/tablegen.gni")

tablegen("IntrinsicEnums") {
  visibility = [ ":tablegen" ]
  args = [ "-gen-intrinsic-enums" ]
  td_file = "Intrinsics.td"
}

tablegen("IntrinsicImpl") {
  visibility = [ ":tablegen" ]
  args = [ "-gen-intrinsic-impl" ]
  td_file = "Intrinsics.td"
}

tablegen("Attributes") {
  visibility = [ ":tablegen" ]
  args = [ "-gen-attrs" ]
}

group("tablegen") {
  public_deps = [
    ":Attributes",
    ":IntrinsicEnums",
    ":IntrinsicImpl",
  ]
}
